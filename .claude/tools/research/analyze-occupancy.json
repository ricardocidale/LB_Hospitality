{
  "name": "analyze_occupancy",
  "description": "Analyze occupancy rate patterns and seasonality for a specific hospitality market. Returns market average occupancy, seasonal patterns with peak/off-peak rates, and expected ramp-up timeline for new properties.",
  "input_schema": {
    "type": "object",
    "properties": {
      "location": {
        "type": "string",
        "description": "The property location (city, state/country)"
      },
      "room_count": {
        "type": "integer",
        "description": "Number of rooms at the property"
      },
      "target_occupancy": {
        "type": "number",
        "description": "Target stabilized occupancy rate (0-1)"
      },
      "property_level": {
        "type": "string",
        "enum": ["budget", "average", "luxury"],
        "description": "Property tier/positioning"
      },
      "catering_level": {
        "type": "string",
        "enum": ["None", "Partial", "Full"],
        "description": "Level of catering/event services"
      }
    },
    "required": ["location", "room_count", "target_occupancy", "property_level"]
  }
}
