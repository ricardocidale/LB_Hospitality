{
  "name": "analyze_management_service_fees",
  "description": "Analyze management company service fee benchmarks for a hotel property. Returns recommended rates for each service fee category (% of Total Revenue) and incentive fee (% of GOP).",
  "input_schema": {
    "type": "object",
    "properties": {
      "location": {
        "type": "string",
        "description": "The property location"
      },
      "room_count": {
        "type": "integer",
        "description": "Number of rooms"
      },
      "property_level": {
        "type": "string",
        "enum": ["budget", "average", "luxury"],
        "description": "Property tier/positioning"
      },
      "has_fb": {
        "type": "boolean",
        "description": "Whether property has F&B operations"
      },
      "has_events": {
        "type": "boolean",
        "description": "Whether property hosts events"
      },
      "market_region": {
        "type": "string",
        "description": "Market region"
      }
    },
    "required": ["location", "room_count", "property_level"]
  }
}
