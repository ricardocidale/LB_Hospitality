{
  "name": "analyze_event_demand",
  "description": "Analyze demand for corporate events, wellness retreats, weddings, and private events in a specific market. Returns estimated revenue potential, key demand drivers, and market positioning insights.",
  "input_schema": {
    "type": "object",
    "properties": {
      "location": {
        "type": "string",
        "description": "The property location"
      },
      "event_locations": {
        "type": "integer",
        "description": "Number of event spaces on property"
      },
      "max_event_capacity": {
        "type": "integer",
        "description": "Maximum event guest capacity"
      },
      "has_wellness": {
        "type": "boolean",
        "description": "Whether property offers wellness programming"
      },
      "has_fb": {
        "type": "boolean",
        "description": "Whether property has F&B operations"
      },
      "acreage": {
        "type": "number",
        "description": "Property acreage"
      },
      "privacy_level": {
        "type": "string",
        "enum": ["low", "moderate", "high"],
        "description": "Property privacy level"
      }
    },
    "required": ["location", "event_locations", "max_event_capacity"]
  }
}
